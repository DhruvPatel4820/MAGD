<% if(success && success.length){ %>
    <div class="row mt-3">
        <div class="alert alert-success col-8 offset-2" role="alert">
            <%= success %>
        </div>
    </div>
    
<%}%>

<% if(error && error.length){ %>
    <div class="row mt-3">
        <div class="alert alert-danger col-8 offset-2" role="alert">
            <%= error %>
        </div>
    </div>
    
<%}%>
